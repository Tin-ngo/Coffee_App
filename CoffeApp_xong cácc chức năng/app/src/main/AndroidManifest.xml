<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coffeapp">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission> <!-- Cho phép sử dụng Internet -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoffeApp">
        <activity android:name=".Quay.QuayActivity"></activity>    <!-- main phải luôn ở đầu  -->
        <activity android:name=".PV.PhucvuActivity"></activity>
        <activity android:name=".ThuNgan.ThunganActivity"></activity>
        <activity android:name=".HomeActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>