<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="20dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/txt_thunhap_giathanh_tennuoc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tên nước"
        android:textSize="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/txt_thunhap_giathanh_gianiemyet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tên nước"
        android:textSize="15dp"
        android:paddingLeft="20dp"
        app:layout_constraintLeft_toRightOf="@+id/txt_thunhap_giathanh_tennuoc"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/txt_thunhap_giathanh_soluong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Số lượng"
        android:textSize="15dp"
        android:paddingLeft="20dp"
        app:layout_constraintLeft_toRightOf="@+id/txt_thunhap_giathanh_gianiemyet"
        app:layout_constraintTop_toTopOf="parent"
        />



</androidx.constraintlayout.widget.ConstraintLayout>